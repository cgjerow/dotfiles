{
  "eslint.autoFixOnSave": true,
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "json", "jsonc", "java"],
  "coc.preferences.jumpCommand": "tab drop",
  "signature.enable": true,
  "signature.preferShownAbove": true,
  "tsserver.maxTsServerMemory": 16000,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-14",
      "path": "/Library/Java/JavaVirtualMachines/jdk-14.jdk/Contents/Home",
      "default": true
    }
  ],
  "java.home": "/Library/Java/JavaVirtualMachines/jdk-14.jdk/Contents/Home",
  "java.import.gradle.wrapper.enabled": false,
  "java.import.gradle.home": true
}
